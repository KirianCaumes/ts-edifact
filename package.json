{
      "name": "ts-edifact",
      "description": "Edifact parser library",
      "version": "0.0.1",
      "type": "module",
      "engines": {
            "vscode": "^1.46.0"
      },
      "publisher": "Roman Vottner",
      "preview": true,
      "readme": "README.md",
      "author": {
            "name": "Roman Vottner"
      },
      "repository": {
            "type": "git",
            "url": "https://github.com/RovoMe/ts-edifact.git"
      },
      "homepage": "https://github.com/RovoMe/ts-edifact/README.md",
      "bugs": {
            "url": "https://github.com/RovoMe/ts-edifact/-/issues",
            "email": "rovo@gmx.at"
      },
      "scripts": {
            "lint": "eslint ./src/main --ext .ts,.tsx",
            "build": "npm run lint && tsc -p .",
            "build:watch": "tsc -p . --watch",
            "check": "npm run check:tsc",
            "check:tsc": "tsc -p . --noEmit",
            "pretest": "npm run build && npm run lint",
            "test": "jasmine-ts JASMINE_CONFIG_PATH=spec/support/jasmine.json",
            "test:alloy": "ts-node -r tsconfig-paths/registernode_modules/jasmine/bin/jasmine.js \"src/test/**/*.spec.ts\"",
            "coverage": "nyc -r lcov -e .ts -x \"*.spec.ts\" jasmine-ts JASMINE_CONFIG_PATH=spec/support/jasmine.json",
            "webpack": "webpack --mode development",
            "webpack-dev": "webpack --mode development --watch",
            "prepackage-dev": "npm run test && npm run webpack",
            "prepackage": "npm run test && webpack --mode production"
      },
      "devDependencies": {
            "@types/jasmine": "^3.5.11",
            "@typescript-eslint/eslint-plugin": "^3.6.1",
            "@typescript-eslint/eslint-plugin-tslint": "^3.6.1",
            "@typescript-eslint/parser": "^3.6.1",
            "edifact": "^1.2.8",
            "eslint": "^6.8.0",
            "eslint-plugin-import": "^2.22.0",
            "eslint-plugin-jsdoc": "^29.2.0",
            "inspect-brk": "^0.1.2",
            "istanbul": "^0.4.5",
            "jasmine": "^3.5.0",
            "jasmine-core": "^3.5.0",
            "jasmine-node": "^3.0.0",
            "jasmine-spec-reporter": "^5.0.2",
            "jasmine-ts": "^0.3.0",
            "jasmine-ts-console-reporter": "^3.1.1",
            "ts-node": "^8.10.2",
            "tsconfig-paths": "^3.9.0",
            "tslint": "^6.1.2",
            "webpack": "^4.43.0",
            "webpack-cli": "^3.3.12"
      },
      "dependencies": {
            "@types/events": "^3.0.0",
            "@types/node": "^14.0.23",
            "events": "^3.1.0",
            "typescript": "^3.9.6"
      }
}
